<ShiyiAsm>
  <AliasSettings>
  </AliasSettings>
  <TargetFileTypes>
    <FileType type="*.js">
      <Alias key="@ShiyiFramework">miniprogram/ShiyiFramework</Alias>
      <Alias key="@Root">miniprogram</Alias>
    </FileType>
    <FileType type="*.sajson" to="*.json" >
    </FileType>
    <FileType type="*.sacss" to="*.wxss" >
      <Alias key="@ShiyiFramework">miniprogram/ShiyiFramework</Alias>
      <Alias key="@Root">miniprogram</Alias>
    </FileType>
    <FileType type="*.saml" to="*.wxml"/>
  </TargetFileTypes>
  <ExcludeSetting>
    <Exclude>miniprogram/miniprogram_npm</Exclude>
  </ExcludeSetting>
  <EnableComponents>true</EnableComponents>
  <Command>
    <BeforeCmd>tsc</BeforeCmd>
  </Command>
</ShiyiAsm>
